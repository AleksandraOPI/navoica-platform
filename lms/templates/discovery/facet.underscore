<h3 class="header-facet">
    <%- gettext(displayName) %>
</h3>
<% if (listIsHuge) { %>
	<select data-facet="<%- name %>" class="facet-list collapse select_list">
		<%= HtmlUtils.ensureHtml(optionsHtml) %>
	</select>
<% } else { %>
	<ul data-facet="<%- name %>" class="facet-list collapse">
		<%= HtmlUtils.ensureHtml(optionsHtml) %>
	</ul>
<% } %>
